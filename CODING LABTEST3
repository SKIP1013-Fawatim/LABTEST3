package labtest3;
import java.util.Scanner;
public class TimeTable{
    public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    
    System.out.print("multiplication table size: ");
      int size =sc.nextInt();
      
      for(int i=1; i<= 10; i++) {
        System.out.print(i + "|");
         for(int j=1; j<=size; j++) {
            System.out.printf("%4d",  i*j );
         }
         System.out.println(); 
      }
     }
    }
     
      
    

         
